ocument.querySelector('#serchFildid');\n        if (searchInput) {\n          // console.log('Search input found, focusing');\n          searchInput.focus();\n        } else {\n          // console.log('Search input not found');\n        }\n      }\n    },\n    setLanguage(locale, country) {\n      this.$i18n.locale = locale;\n      this.current_language = locale;\n      this.text = country;\n    },\n    changeMode(mode) {\n      this.currentMode = mode;\n      if (mode === \"dark\") {\n        document.body.setAttribute(\"data-pc-theme\", \"dark\");\n        document.body.setAttribute(\"data-topbar\", \"dark\");\n        document.body.classList.remove(\"mode-auto\");\n      } else if (mode === \"auto\") {\n        document.body.setAttribute(\"data-pc-theme\", \"light\");\n        document.body.setAttribute(\"data-topbar\", \"light\");\n        document.body.classList.add(\"mode-auto\");\n      } else {\n        document.body.setAttribute(\"data-pc-theme\", \"light\");\n        document.body.setAttribute(\"data-topbar\", \"light\");\n        document.body.classList.remove(\"mode-auto\");\n      }\n    },\n    toggleSidebar() {\n      this.$store.commit('toggleSidebar');\n    },\n    toggleMobileSidebar() {\n      this.$store.commit('toggleMobileSidebar');\n    },\n    toggleRightSidebar() {\n      this.$emit('toggleRightSidebar'); // Emit an event to toggle the right sidebar\n    }\n  }\n};","map":{"version":3,"names":["simplebar","rightBar","name","components","data","isFullscreen","isSidebarHidden","currentMode","show","languages","language","title","subtitle","current_language","$i18n","locale","text","value","mounted","document","addEventListener","handleKeyDown","find","x","beforeUnmount","removeEventListener","methods","handleSearch","event","preventDefault","ctrlKey","key","searchInput","querySelector","focus","setLanguage","country","changeMode","mode","body","setAttribute","classList","remove","add","toggleSidebar","$store","commit","toggleMobileSidebar","t