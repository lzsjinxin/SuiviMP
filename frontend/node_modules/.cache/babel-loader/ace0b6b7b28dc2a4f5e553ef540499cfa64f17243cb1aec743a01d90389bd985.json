{"ast":null,"code":"import Layout from \"@/layout/main.vue\";\nimport pageheader from \"@/components/page-header.vue\";\nimport axios from \"axios\";\nexport default {\n  name: \"SAMPLE-PAGE\",\n  components: {\n    Layout,\n    pageheader\n  },\n  data() {\n    return {\n      items: []\n    };\n  },\n  methods: {\n    fetchData() {\n      axios.get('/api/arrivals/').then(response => {\n        this.items = response.data;\n      }).catch(error => {\n        console.error(error);\n      });\n    }\n  },\n  mounted() {\n    this.fetchData();\n  },\n  computed: {\n    formattedDate() {\n      const date = new Date(this.item.date);\n      const day = String(date.getDate()).padStart(2, '0');\n      const month = String(date.getMonth() + 1).padStart(2, '0'); // Months are zero-based\n\n      const year = date.getFullYear();\n      const hours = String(date.getHours()).padStart(2, '0');\n      const minutes = String(date.getMinutes()).padStart(2, '0');\n      return `${day}/${month}/${year} ${hours}:${minutes}`;\n    }\n  }\n};","map":{"version":3,"names":["Layout","pageheader","axios","name","components","data","items","methods","fetchData","get","then","response","catch","error","console","mounted","computed","formattedDate","date","Date","item","day","String","getDate","padStart","month","getMonth","year","getFullYear","hours","getHours","minutes","getMinutes"],"sources":["/home/lzsjinxin/Documents/Dev/SuiviMP/frontend/src/views/ArrivalList.vue"],"sourcesContent":["<script>\r\nimport Layout from \"@/layout/main.vue\"\r\nimport pageheader from \"@/components/page-header.vue\"\r\nimport axios from \"axios\"\r\nexport default {\r\n    name: \"SAMPLE-PAGE\",\r\n    components: {\r\n        Layout, pageheader\r\n    },\r\n    data(){\r\n    return{\r\n        items : []\r\n    }\r\n    },\r\n    methods: {\r\n    fetchData() {\r\n      axios.get('/api/arrivals/')\r\n        .then(response => {\r\n          this.items = response.data;\r\n        })\r\n        .catch(error => {\r\n          console.error(error);\r\n        });\r\n    }\r\n  },\r\n  mounted() {\r\n    this.fetchData();\r\n  },\r\n  computed: {\r\n\r\nformattedDate() {\r\n\r\n  const date = new Date(this.item.date);\r\n\r\n  const day = String(date.getDate()).padStart(2, '0');\r\n\r\n  const month = String(date.getMonth() + 1).padStart(2, '0'); // Months are zero-based\r\n\r\n  const year = date.getFullYear();\r\n\r\n  const hours = String(date.getHours()).padStart(2, '0');\r\n\r\n  const minutes = String(date.getMinutes()).padStart(2, '0');\r\n\r\n\r\n  return `${day}/${month}/${year} ${hours}:${minutes}`;\r\n\r\n}\r\n  }\r\n}\r\n</script>\r\n\r\n<template>\r\n    <Layout>\r\n        <pageheader title=\"Liste des Arrivages\" :pageTitle=\"$t('Arrivals')\" />\r\n\r\n        <BRow>\r\n            <BCol sm=\"12\">\r\n                <BCard no-body>\r\n                    <BCardBody>\r\n                        <div class=\"table-responsive\">\r\n                            <table class=\"table table-hover tbl-product\" id=\"pc-dt-simple\">\r\n                                <thead>\r\n                                    <tr > \r\n                                        <th class=\"text-end\">#</th>\r\n                                        <th>Date</th>\r\n                                        <th>Provenance</th>\r\n                                        <th >Matricule de Vehicule</th>\r\n                                        <th >Receptionn√© Par</th>\r\n                                    </tr>\r\n                                </thead>\r\n                                <tbody>\r\n                                    <tr  v-for=\"item in items\" :key=\"item.id\" style=\"cursor: pointer;\">\r\n                                        <td class=\"text-end\">{{ item.id }}</td>\r\n                                        <td> {{ item.date }} </td>\r\n                                        <td>{{ item.tier }}</td>\r\n                                        <td>{{ item.vehicule_registration }}</td>\r\n                                        <td class=\"text-center\">\r\n                                            <router-link :to=\"`users/${item.useradd}`\" class=\"b-brand text-primary\"> {{ item.user }} </router-link>\r\n                                            <!-- <div class=\"prod-action-links\">\r\n                                                <ul class=\"list-inline me-auto mb-0\">\r\n                                                    <li class=\"list-inline-item align-bottom\" data-bs-toggle=\"tooltip\"\r\n                                                        title=\"View\">\r\n                                                        <a href=\"#\"\r\n                                                            class=\"avtar avtar-xs btn-link-secondary btn-pc-default\"\r\n                                                            data-bs-toggle=\"offcanvas\"\r\n                                                            data-bs-target=\"#productOffcanvas\">\r\n                                                            <i class=\"ti ti-eye f-18\"></i>\r\n                                                        </a>\r\n                                                    </li>\r\n                                                    <li class=\"list-inline-item align-bottom\" data-bs-toggle=\"tooltip\"\r\n                                                        title=\"Edit\">\r\n                                                        <router-link to=\"/arrivals\"\r\n                                                            class=\" avtar avtar-xs btn-link-success btn-pc-default\">\r\n                                                            <i class=\"ti ti-edit-circle f-18\"></i>\r\n                                                        </router-link>\r\n                                                    </li>\r\n                                                    <li class=\"list-inline-item align-bottom\" data-bs-toggle=\"tooltip\"\r\n                                                        title=\"Delete\">\r\n                                                        <a href=\"#\"\r\n                                                            class=\"avtar avtar-xs btn-link-danger btn-pc-default\">\r\n                                                            <i class=\"ti ti-trash f-18\"></i>\r\n                                                        </a>\r\n                                                    </li>\r\n                                                </ul>\r\n                                            </div> -->\r\n                                        </td>\r\n                                    </tr>\r\n                                </tbody>\r\n                            </table>\r\n                        </div>\r\n                    </BCardBody>\r\n                </BCard>\r\n            </BCol>\r\n        </BRow>\r\n    </Layout>\r\n</template>"],"mappings":"AACA,OAAOA,MAAK,MAAO,mBAAkB;AACrC,OAAOC,UAAS,MAAO,8BAA6B;AACpD,OAAOC,KAAI,MAAO,OAAM;AACxB,eAAe;EACXC,IAAI,EAAE,aAAa;EACnBC,UAAU,EAAE;IACRJ,MAAM;IAAEC;EACZ,CAAC;EACDI,IAAIA,CAAA,EAAE;IACN,OAAM;MACFC,KAAI,EAAI;IACZ;EACA,CAAC;EACDC,OAAO,EAAE;IACTC,SAASA,CAAA,EAAG;MACVN,KAAK,CAACO,GAAG,CAAC,gBAAgB,EACvBC,IAAI,CAACC,QAAO,IAAK;QAChB,IAAI,CAACL,KAAI,GAAIK,QAAQ,CAACN,IAAI;MAC5B,CAAC,EACAO,KAAK,CAACC,KAAI,IAAK;QACdC,OAAO,CAACD,KAAK,CAACA,KAAK,CAAC;MACtB,CAAC,CAAC;IACN;EACF,CAAC;EACDE,OAAOA,CAAA,EAAG;IACR,IAAI,CAACP,SAAS,CAAC,CAAC;EAClB,CAAC;EACDQ,QAAQ,EAAE;IAEZC,aAAaA,CAAA,EAAG;MAEd,MAAMC,IAAG,GAAI,IAAIC,IAAI,CAAC,IAAI,CAACC,IAAI,CAACF,IAAI,CAAC;MAErC,MAAMG,GAAE,GAAIC,MAAM,CAACJ,IAAI,CAACK,OAAO,CAAC,CAAC,CAAC,CAACC,QAAQ,CAAC,CAAC,EAAE,GAAG,CAAC;MAEnD,MAAMC,KAAI,GAAIH,MAAM,CAACJ,IAAI,CAACQ,QAAQ,CAAC,IAAI,CAAC,CAAC,CAACF,QAAQ,CAAC,CAAC,EAAE,GAAG,CAAC,EAAE;;MAE5D,MAAMG,IAAG,GAAIT,IAAI,CAACU,WAAW,CAAC,CAAC;MAE/B,MAAMC,KAAI,GAAIP,MAAM,CAACJ,IAAI,CAACY,QAAQ,CAAC,CAAC,CAAC,CAACN,QAAQ,CAAC,CAAC,EAAE,GAAG,CAAC;MAEtD